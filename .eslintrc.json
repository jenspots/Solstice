{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "semi": "off",
    "@typescript-eslint/semi": ["error"],
    "eol-last": ["error", "always"],
    "comma-dangle": ["error", "always-multiline"],
    "@typescript-eslint/ban-ts-comment": "off"
  }
}
